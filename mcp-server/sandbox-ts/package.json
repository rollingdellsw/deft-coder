{
  "name": "mcp-server-sandbox-ts",
  "version": "1.0.0",
  "description": "TypeScript sandbox MCP server",
  "main": "dist/index.js",
  "bin": {
    "search": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint src --ext .ts --cache",
    "lint:fix": "eslint src --ext .ts --cache --fix",
    "type-check": "tsc --noEmit",
    "build": "tsc --noEmit && node build.js",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "esbuild": "^0.27.1",
    "express": "^4.18.2",
    "puppeteer": "^24.33.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "tsx": "^4.7.1"
  }
}

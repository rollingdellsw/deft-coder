{
  "name": "git",
  "version": "1.0.0",
  "description": "MCP server for git operations",
  "main": "dist/index.js",
  "bin": {
    "git": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint src --ext .ts --cache",
    "lint:fix": "eslint src --ext .ts --cache --fix",
    "type-check": "tsc --noEmit",
    "build": "tsc --noEmit && node build.js",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js"
  },
  "keywords": [
    "mcp",
    "git",
    "version-control"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.27.1",
    "eslint": "^8.54.0",
    "fast-glob": "^3.3.3",
    "typescript": "^5.3.2"
  }
}
